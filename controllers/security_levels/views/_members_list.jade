if members
  div.table-responsive
    table.table.table-hover
      thead
        tr
          th Index
          th Account Name
          th Project
      tbody
        each member, index in members
          - var account = member.account;
          - var project = member.project;
          tr
            td=index + 1
            td: a(href='/projects/' + project.id + '/member/' + account.id)=account.name
            td: a(href='/projects/' + project.id)=project.name
